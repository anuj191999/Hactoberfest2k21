#include<bits/stdc++.h>
using namespace std;

struct Node
{
	int data;
	struct Node *left;
	struct Node *right;
	Node(int k)
	{
		data=k;
		left=right=NULL;
	}
};

int ceil_key(Node *root,int key)
{
   int res=INT_MAX;
   while(root!=NULL)
   {
   	  if(root->data==key)
   	   {
   	      res=root->data;
			 break;	
	   }
   	  else
   	  {
   	      if(root->data>key)
   	      {
   	         	res=min(res,root->data);
   	         	root=root->left;
		  }
		  else
		  {
		  	 root=root->right;
		  }
			 	
	  }
   	   
   }
   return res;
}
int main()
{
	Node *root;
	root=new Node(8);
	root->left=new Node(3);
	root->left->right=new Node(6);
	root->left->left=new Node(1);
	root->right=new Node(10);
	root->right->right=new Node(14);
	
	cout<<"Ceil value of 13 is "<<ceil_key(root,13)<<endl;
}
